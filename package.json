    "promise-wtf": "1.x.x",
    "xml2json": "0.x.x"
  },
  "devDependencies": {
    "coveralls": "latest",
    "eslint": "latest",
    "eslint-config-ggc": "latest",
    "nock": "^8.0.0",
    "nyc": "latest",
    "snyk": "latest",
    "tap-spec": "latest",
    "tape": "latest"
  },
  "keywords": [
    "extractor",
    "parser",
    "feed",
    "rss",
