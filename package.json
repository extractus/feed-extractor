{
  "version": "1.0.1",
  "name": "feed-reader",
  "description": "Handle ATOM and RSS feed",
  "homepage": "https://www.npmjs.com/package/feed-reader",
  "repository": {
    "type": "git",
    "url": "https://github.com/ndaidong/feed-reader"
  },
  "author": "@ndaidong",
  "main": "./index.js",
  "engines": {
    "node": ">= 6.0"
  },
  "scripts": {
    "lint": "eslint .",
    "snyk": "snyk test",
    "tape": "nyc tape test/start.js | tap-spec",
